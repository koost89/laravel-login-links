{
    "name": "koost89/laravel-otl",
    "description": "This package allows you to generate a one time login link to log in as a user",
    "keywords": [
        "koost89",
        "laravel",
        "one-time-login",
        "user",
        "login",
        "link",
        "user-login",
        "authenticate",
        "passwordless"
    ],
    "homepage": "https://github.com/koost89/laravel-otl",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Oosterveen",
            "email": "kevinoosterveen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "illuminate/auth": "^6.0 || ^7.0 || ^8.0",
        "illuminate/console": "^6.0 || ^7.0 || ^8.0",
        "illuminate/container": "^6.0 || ^7.0 || ^8.0",
        "nesbot/carbon": "^2.35"
    },
    "require-dev": {
        "nunomaduro/collision": "^3.0 || ^4.0 || ^5.0",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Koost89\\UserLogin\\": "src",
            "Koost89\\UserLogin\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Koost89\\UserLogin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --no-coverage",
        "test-coverage": "XDEBUG_MODE=coverage ./vendor/bin/testbench package:test --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Koost89\\UserLogin\\UserLoginServiceProvider"
            ],
            "aliases": {
                "UserLogin": "Koost89\\UserLogin\\Facades\\UserLogin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
